  }
  
  #hp_intro, #hp_intro a, #quiz_header, .idea_position, .your_idea_position, .profile_name, .profile_name a, h1, h2, h3, h4 {
  	color: ##{scheme.heading}; /* 000 */
  }
  .subhead, .subhead a, .heading, .heading a, .idea_chart td, .table_header, .home_issue_name, .home_issue_name a, .index_chart_legend, #issue_nav_name {
  	color: ##{scheme.sub_heading}; /* 999 */
  }
  
  .greyed_out, .comment_hidden, .comment_hidden a, .nochange, .position_nochange, .activity_hidden, .activity_hidden a, .show_all_encouragements a, .small, .tag_count {
  	color: ##{scheme.greyed_out}; /* 999 */
  }
  
  #idea_column, #top_right_column {
  	background-color: ##{scheme.box}; /* f0f0f0 */
  	color: ##{scheme.box_text}; /* 444 */	
  }
  
  #intro, #buzz_box, #not_activated, #notification_show, #errorExplanation, .bulletin_form, .change_vote_now, #contacts, #contacts_small, .invite_text, .notice {
  	background-color: ##{scheme.box}; /* f0f0f0 */
  	color: ##{scheme.box_text}; /* 444 */
  }
  
  #footer_container {
  	background-color: ##{scheme.footer}; /* f0f0f0 */
  	color: ##{scheme.footer_text}; /* 444 */	
  }
  
  .comment, .comment_endorses, .comment_opposes, .comment_more, .comment_form, .comment_link_add, .comment_flag, .comment_hidden, .message, .message_link_add, .message_form {
  	background-color: ##{scheme.comments}; /* f0f0f0 */
  	color: ##{scheme.comments_text};
  }
  
  input, textarea, #contacts, #contacts_small, .invite_text, .ac_results, .magnifier_wrap, .change_vote_now, .idleField {
  	background-color: ##{scheme.input};
  	border: solid 3px ##{scheme.border}; /* ddd */
  }
  
  .focusField{  
      border: solid 3px ##{scheme.link};  
  }  
  
  .endorsement_down, .endorsement_up, .pc, .profile_pc {
  	border: dotted 1px ##{scheme.border}; /* ddd */
  }
  
  .pc, .pc a, .pc_under, .pc_under a, .profile_pc, .profile_pc a {
  	color: ##{scheme.greyed_out}; /* 999 */
  }
  .comment_x a, .endorsement_x a, .activity_x a, .x a {
  	color: ##{scheme.greyed_out}; /* 999 */
  }
  .activity_border, .activity_hidden {
  	border-top: dotted 1px ##{scheme.border}; /* ddd */
  }
  .grey_button_wide, .import_button, .notice {
  	background-color: ##{scheme.grey_button};	/* f0f0f0 */
  	border: solid 3px ##{scheme.grey_button_border};	 /* ddd */	
  	color: ##{scheme.grey_button_text};	
  }
  .tag_button, .grey_button {
  	background-color: ##{scheme.grey_button};	/* f0f0f0 */
  	border: solid 1px ##{scheme.grey_button_border};	 /* ddd */	
  	color: ##{scheme.grey_button_text};
  }
  .tag_button a, .grey_button a {
  	color: ##{scheme.grey_button_text};
  }
  
  .tag_button:hover, .grey_button:hover {
  	border: solid 1px ##{scheme.link};
  }
  
  .message_error, .comment_error, .error, #prioritizer_changes, .form_errors, .position_new, .new, .red, .cancel a, #status, #login_message {
  	color: ##{scheme.error}; /* bc0000 */
  }
  
  .flash_error, #not_activated_header, #warning, .fieldWithErrors input, #errorExplanation h2 {
  	background-color: ##{scheme.error}; /* bc0000 */
  	color: ##{scheme.error_text}; /* fff */
  	font-weight: bold;
  }
  #not_activated, #errorExplanation {
  	border: solid 3px ##{scheme.error}; /* bc0000 */
  }
  
  .gainer {
  	color: ##{scheme.up}; /* 009933 */
  }
  .loser {
  	color: ##{scheme.down};
  }
  .green_button, .idea_remove, .endorsed, .flash_notice {
  	background-color: ##{scheme.endorsed_button}; /* 009933 */
  }
  .flash_notice, .green_button, .green_button a, .idea_remove, .idea_remove a, .endorsed, .endorsed a {
  	color: ##{scheme.endorsed_button_text}; /* FFF */
  }
  
  .opposed, .red_button, .idea_remove_oppose {
  	background-color: ##{scheme.opposed_button}; /* bc0000 */
  }
  .opposed, .opposed a, .red_button, .red_button a, .idea_remove_oppose, .idea_remove_oppose a, .close_notify a, .close_notify {
  	color: ##{scheme.opposed_button_text}; /* FFF */
  }
  
  .suggest_button, .yellow_button {
  	background-color: ##{scheme.action_button}; /* FFFF99 */
  	border: solid 3px ##{scheme.action_button_border};
  	color: ##{scheme.action_button_text};
  }
  .suggest_button a, .yellow_button a {
  	color: ##{scheme.action_button_text};	
  }
  .idea_add {
  	background-color: ##{scheme.action_button}; /* FFFF99 */
  	border: solid 1px ##{scheme.action_button_border};		
  }
  .idea_add a {
      color: ##{scheme.action_button_text};
  }
  .suggest_button:hover, .yellow_button:hover, .grey_button_wide:hover {
  	border: solid 3px ##{scheme.link};
  }
  
  .compromised {
  	background-color: ##{scheme.compromised_button}; /* ffcc00 */
  }
  .compromised, .compromised a {
  	color: ##{scheme.compromised_button_text}; /* FFF */
  }
  
  .pagination .current {
  	background-color: ##{scheme.nav_background};
  	color: ##{scheme.nav_text};
  }
  .ajax_pagination .current {
  	background-color: ##{scheme.nav_selected_background};
  	color: ##{scheme.nav_selected_text};
  }
  
