:css
  body {
  	background-color: ##{scheme.footer}; /* fff */
  	color: ##{scheme.heading}; /* 000 */
  	font-family: #{scheme.fonts};
  <haml:silent>	 if scheme.attribute_present?("background_image_file_name") 
  </haml:silent><haml:block>		background-image: url(#{scheme.background_image.url});
  </haml:block><haml:silent>	 if scheme.background_tiled? 
  </haml:silent><haml:block>		background-repeat: repeat;
  </haml:block>}
  #backgroundwrap {
  	background-color: ##{scheme.background};
  <haml:silent>	 if scheme.attribute_present?("background_image_file_name") 
  </haml:silent><haml:block>		background-image: url(#{scheme.background_image.url});
  </haml:block><haml:silent>	 if scheme.background_tiled? 
  </haml:silent><haml:block>		background-repeat: repeat;
  </haml:block>}
  a, .endorsement_down, .tag_button, .yellow_button, .grey_button_wide {
  	color: ##{scheme.link};	/* 13499b */
  }
  
  #content_text {
  	color: ##{scheme.text}; /* 444 */
