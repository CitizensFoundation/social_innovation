- if Instance.current.message_for_finished and Instance.current.message_for_finished!=""
  = render :partial=>"/shared/top_message_box", :locals=> {:message_text => Instance.current.message_for_finished.html_safe }
%div{:style => "width:auto;"}
  %ul
    %li.list_left
      #hp_intro= @page_title
    %li.list_right
      = rss_url(@rss_url)
  %ul
.clear
.white_line
- for priority in @priorities
  = render :partial => "priorities/listing_finished", :locals => {:priority => priority }
= will_paginate @priorities
