#branding{:style => "height: 85px"}
  %table
    %tr
      %td
        #main_logo
          = link_to :controller=>"portal", :host=>request.host do
            = image_tag "/assets/new/betraIsland-merki_340x74.png"
      - if false and current_sub_instance
        %td
          #sub_instance_logo
            = render :partial => "sub_instances/logo"
      %td
